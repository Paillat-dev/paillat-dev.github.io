    "@lucide/svelte": "^0.563.1",
    "@tailwindcss/typography": "^0.5.19",
    "@tailwindcss/vite": "^4.1.18",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "astro": "^5.17.1",
    "bits-ui": "^2.14.4",
    "clsx": "^2.1.1",
    "eslint": "^9.39.2",
