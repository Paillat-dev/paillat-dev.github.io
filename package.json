{
  "name": "",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "format:prettier": "prettier --write .",
    "format:md": "pnpm run format:md:check -o",
    "format:mdx": "pnpm run format:mdx:check -o",
    "format:md:check": "remark src *.md --frail --ext .md --rc-path .remarkrc.md.json",
    "format:mdx:check": "remark src --frail --ext .mdx --rc-path .remarkrc.mdx.json",
    "format:prettier:check": "prettier --check .",
    "format": "npm-run-all format:prettier format:md format:mdx",
    "format:check": "npm-run-all format:prettier:check format:md:check format:mdx:check",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck:astro": "astro check",
    "typecheck:ts": "tsc --noEmit",
    "typecheck:svelte": "svelte-check --no-tsconfig --ignore src/lib/components/ui",
    "typecheck": "npm-run-all \"astro sync\" typecheck:*"
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.6",
    "@astrojs/mdx": "^4.3.13",
