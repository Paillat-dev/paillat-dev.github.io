{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@components": ["./src/components"],
      "@components/*": ["./src/components/*"],
      "@layouts": ["./src/layouts"],
      "@layouts/*": ["./src/layouts/*"],
      "@content": ["./src/content"],
      "@content/*": ["./src/content/*"],
      "@lib": ["./src/lib"],
      "@lib/*": ["./src/lib/*"],
      "@stores": ["./src/stores"],
      "@stores/*": ["./src/stores/*"]
    }
  }
}
